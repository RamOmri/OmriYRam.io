{
  "name": "omriyram",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --web",
    "build": "expo export:web",
    "docker:build": "source .env && docker build -t $DOCKER_ID .",
    "docker:start:local": "source .env && docker run -p 8080:8080 $DOCKER_ID",
    "submit": "source .env && yarn build && gcloud builds submit --tag gcr.io/$GOOGLE_PROJECT/$DOCKER_ID .",
    "deploy": "source .env && yarn submit && gcloud run deploy --image gcr.io/$GOOGLE_PROJECT/$DOCKER_ID:latest --platform managed"
  },
  "dependencies": {
    "@expo/webpack-config": "^0.17.2",
    "@react-native-masked-view/masked-view": "0.2.8",
    "@react-navigation/material-top-tabs": "^6.5.1",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/stack": "^6.3.10",
    "@reduxjs/toolkit": "^1.9.1",
    "contentful": "^9.2.14",
    "expo": "~47.0.9",
    "expo-linear-gradient": "~12.0.1",
    "expo-status-bar": "~1.4.2",
    "fbjs": "^3.0.4",
    "lodash": "^4.17.21",
    "metro-core": "^0.73.6",
    "react": "18.1.0",
    "react-dom": "^18.2.0",
    "react-native": "0.70.5",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-pager-view": "6.0.1",
    "react-native-reanimated": "^2.13.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-tab-view": "^3.3.4",
    "react-native-web": "^0.18.10",
    "react-router-dom": "^6.6.1",
    "react-visibility-sensor": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/fbjs": "^3.0.4",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-native": "~0.70.6",
    "babel-preset-expo": "^9.2.2",
    "dotenv": "^16.0.3",
    "typescript": "^4.6.3"
  },
  "private": true
}
